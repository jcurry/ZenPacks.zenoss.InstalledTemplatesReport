            <div class="col-xs-3 bs-docs-sidebar">
                <ul class="nav nav-tabs nav-stacked fixed">
                    <h4>ZenPacks: Table of Contents</h4>
                    <tal:block tal:repeat="t objects">
                        <tal:block tal:repeat="r t/zenpack_sort">
