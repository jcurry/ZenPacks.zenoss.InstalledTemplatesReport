<!-- <script src = "https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.1.135/jspdf.debug.js"></script>
<script src = "https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.1.135/jspdf.min.js"></script> -->
<style type="text/css">
.bs-example{
    margin : 20px;
    margin-top: 60px;
}
.group{
    padding-top: 60px;
    margin-top: -60px;
}
.bs-docs-sidebar>ul{
    position: fixed;
    overflow-y: auto;
    height: 100%;
    padding-bottom: 60px; 
}
.bs-docs-content{
    position: fixed;
    overflow-y: auto; 
    height: 100%;
    padding-bottom: 60px;
}
.space_22{
    margin-left: 22px;
}

</style>
<script>

$(document).ready(function(){

    var data;
    $('#sort_by li').click(function(e)
   {
       data = $(this).text();
   });
    var specialElementHandlers = {
        '#editor': function (element,renderer) {
            return true;
        }
    };
 $('#cmd').click(function () {
        var hmtl_content;
        if(data=='ZenPack Name')
        {
            hmtl_content = $('#zenpack_data').html();
            data = 'ZenPack_Name';
        }
        else if(data=='Device Class')
        {
            hmtl_content = $('#device_data').html();
            data = 'Device_Class';
        }
        else
        {
            data = 'Template_Name'
            hmtl_content = $('#template_data').html();

        }
         var doc = new jsPDF();

         doc.fromHTML(hmtl_content, 15, 15, {
             'width': 170,'elementHandlers': specialElementHandlers
         });

         var currentDate = new Date();
         var datetime = (currentDate.getMonth()+1) + "/"
                 + currentDate.getDate()  + "/"
                 + currentDate.getFullYear() + "@"
                 + currentDate.getHours() + ":"
                 + currentDate.getMinutes() + ":"
                 + currentDate.getSeconds();

         data = data + '-' + datetime
         doc.save(data + '.pdf');
    });
});
</script>
