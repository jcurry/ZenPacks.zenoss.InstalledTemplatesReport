<body data-spy="scroll" data-target="#myScrollspy">
<div class="bs-example">
    <nav id="myNavbar" class="navbar navbar-default navbar-fixed-top" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header nav">
            <a class="navbar-brand">Monitoring Capabilites Report</a>
        </div>
        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav navbar-right">
                <li class="dropdown">
                    <a href="#" data-toggle="dropdown" class="dropdown-toggle">Sort by:   <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <tal:block tal:repeat="t objects">
                            <li><a data-toggle="tab" href="#template_category">Template Name</a></li>
                            <li><a data-toggle="tab" href="#zenpack_category">ZenPack Name</a></li>
                            <li><a data-toggle="tab" href="#device_category">Device Class</a></li>
                        </tal:block>
                    </ul>
                </li>
            </ul>
        </div><!-- /.navbar-collapse -->
        
    </nav>
    <div class="row tab-content">

        <!-- Template Category -->

        <div id="template_category" class="tab-pane fade in active">
            <div class="col-xs-3 bs-docs-sidebar">
                <ul class="nav nav-tabs nav-stacked fixed" id="myScrollspy">
                    <h4>Templates: Table of Contents</h4>
                    <tal:block tal:repeat="t objects">
                        <tal:block tal:repeat="r t/template_sort">
                            <li tal:condition="r/primary"><a href="?" tal:attributes="href string:#${r/primary}"><span tal:content="r/primary"/></a>
                            </li>
                        </tal:block>
                    </tal:block>
                </ul>
            </div>
            <div class="col-xs-9 tab-content">
                <tal:block tal:repeat="t objects">

                    <tal:block tal:repeat="r t/template_sort">

                        <h3 id="?" tal:attributes="id string:${r/primary}" class="group"><strong tal:content="r/primary"/></h3>
                        <hr>
                        <p><strong>Device Class:</strong><em tal:content="r/secondary"/></p>
                        <p><strong>ZenPack Name:</strong><em tal:content="r/tertiary"/></p>
                        <p tal:condition = "r/template_description"><strong>Description:</strong> <em tal:content="r/template_description"/></p>
                        
                        
                        <p tal:condition = "r/datasources_info"><strong>Data Sources:</strong></p>
                        <!-- DataSources -->
                        <tal:block tal:repeat="ds r/datasources_info">
                            <p>DataSource: <em tal:content="ds/datasource_id"/>;&nbsp;&nbsp; SSH Enabled: <em tal:content="ds/datasource_ssh_enabled"/>;&nbsp;&nbsp; Type: <em tal:content="ds/datasource_type"/></p>
                            <p tal:condition="ds/datasource_description">&nbsp;&nbsp;&nbsp;&nbsp; Description: <em tal:content="ds/datasource_description"/></p>

                             <tal:block tal:repeat="dp ds/datapoints_info">
                                <p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; DataPoint: <em tal:content="dp/datapoint_id"/>;&nbsp;&nbsp; Type: <em tal:content="dp/datapoint_type"/><span tal:condition="dp/datapoint_alias">;&nbsp;&nbsp; Alias: <em tal:content="dp/datapoint_alias"/></span></p>
                                <p tal:condition="dp/datapoint_description">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Description: <em tal:content="dp/datapoint_description"/></p>
                             </tal:block>
                        </tal:block>

                        
                        <p tal:condition = "r/thresholds_info"><strong>Thresholds:</strong></p>
                        <!-- Thresholds -->
                        <tal:block tal:repeat="th r/thresholds_info">
                            <p>Name: <em tal:content="th/threshold_id"/>;&nbsp;&nbsp;Type: <em tal:content="th/threshold_type"/><span tal:condition="th/threshold_min">;&nbsp;&nbsp; Min: <em tal:content="th/threshold_min"/></span><span tal:condition="th/threshold_max">;&nbsp;&nbsp;Max: <em tal:content="th/threshold_max"/></span></p>
                            <p tal:condition="th/threshold_description">&nbsp;&nbsp;&nbsp;&nbsp;Description: <em tal:content="th/threshold_description"/></p>
                        </tal:block>
                        

                        <p tal:condition = "r/graphdefs_info"><strong>Graph Definitions:</strong></p>
                        <!-- GraphDefs -->
                        <tal:block tal:repeat="gd r/graphdefs_info">
                            <p>Name: <em tal:content="gd/graphDef_id"/><span tal:condition="gd/graphDef_unit">; &nbsp; Unit: <em tal:content="gd/graphDef_unit"/></span></p>
                            <p tal:condition="gd/graphDef_description">&nbsp;&nbsp;&nbsp;&nbsp; Description: <em tal:content="gd/graphDef_description"/></p>
                                <tal:block tal:repeat="gp gd/graphpoints_info">
                                    <p>&nbsp;&nbsp; GraphPoints: <em tal:content="gp/graphPoint_id"/>; &nbsp; Type: <em tal:content="gp/graphPoint_type"/></p>
                                    <p tal:condition="gp/graphPoint_description">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Description: <em tal:content="gp/graphPoint_description"/></p>
                                </tal:block>
                        </tal:block>

                        <p>&nbsp;</p>
                        <p>&nbsp;</p>

                    </tal:block>
                </tal:block>
            </div>
        </div>

